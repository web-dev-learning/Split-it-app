{
	"name": "split-it",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-modal": "2.2.2",
		"react-redux": "5.0.5",
		"react-router-dom": "4.2.2",
		"react-scripts": "3.0.1",
		"redux": "3.7.2",
		"uuid": "^3.3.2"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"format-check": "prettier --check \"src/**/*.{js,jsx,json,css,scss}\"",
		"format": "prettier --write \"src/**/*.{js,jsx,json,css,scss}\""
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"husky": "^3.0.2",
		"prettier": "^1.18.2",
		"pretty-quick": "^1.11.1",
		"sass": "^1.22.3"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged --verbose"
		}
	}
}
